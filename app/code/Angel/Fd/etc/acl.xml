<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Angel_Core::angel">
					<resource id="Angel_Core::fd" title="50/50 Raffle">
						<resource id="Angel_Fd::Product" sortOrder="10" title="50/50 Product Management"/>
						<resource id="Angel_Fd::Ticket" sortOrder="10" title="Ticket">
							<resource id="Angel_Fd::Ticket_save" sortOrder="10" title="Save Ticket"/>
							<resource id="Angel_Fd::Ticket_delete" sortOrder="20" title="Delete Ticket"/>
							<resource id="Angel_Fd::Ticket_update" sortOrder="30" title="Update Ticket"/>
							<resource id="Angel_Fd::Ticket_view" sortOrder="40" title="View Ticket"/>
							<resource id="Angel_Fd::Ticket_print" sortOrder="40" title="Print Ticket"/>
						</resource>
					</resource>
				</resource>

				<resource id="Magento_Sales::sales">
					<resource id="Angel_Fd::report" sortOrder="10" title="50/50 Tickets Report"/>
				</resource>

				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Angel_Fd::config_angel_fd" title="50/50 Raffle"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
